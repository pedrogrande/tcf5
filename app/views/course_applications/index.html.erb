<h1>Listing course_applications</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Intake</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Reason</th>
      <th>Experience</th>
      <th>Education</th>
      <th>Age</th>
      <th>City</th>
      <th>Country</th>
      <th>Employed</th>
      <th>Referral</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_applications.each do |course_application| %>
      <tr>
        <td><%= course_application.course %></td>
        <td><%= course_application.intake %></td>
        <td><%= course_application.first_name %></td>
        <td><%= course_application.last_name %></td>
        <td><%= course_application.email %></td>
        <td><%= course_application.phone %></td>
        <td><%= course_application.reason %></td>
        <td><%= course_application.experience %></td>
        <td><%= course_application.education %></td>
        <td><%= course_application.age %></td>
        <td><%= course_application.city %></td>
        <td><%= course_application.country %></td>
        <td><%= course_application.employed %></td>
        <td><%= course_application.referral %></td>
        <td><%= link_to 'Show', course_application %></td>
        <td><%= link_to 'Edit', edit_course_application_path(course_application) %></td>
        <td><%= link_to 'Destroy', course_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course application', new_course_application_path %>
