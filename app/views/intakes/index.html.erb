<h1>Listing intakes</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Location</th>
      <th>Day</th>
      <th>Date</th>
      <th>End date</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Number of places</th>
      <th>Number of students registered</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intakes.each do |intake| %>
      <tr>
        <td><%= intake.course.name %></td>
        <td><%= intake.location.name %></td>
        <td><%= intake.day %></td>
        <td><%= intake.date %></td>
        <td><%= intake.end_date %></td>
        <td><%= intake.start_time %></td>
        <td><%= intake.end_time %></td>
        <td><%= intake.number_of_places %></td>
        <td><%= intake.number_of_students_registered %></td>
        <td><%= intake.active %></td>
        <td><%= link_to 'Show', intake %></td>
        <td><%= link_to 'Edit', edit_intake_path(intake) %></td>
        <td><%= link_to 'Destroy', intake, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intake', new_intake_path %>
