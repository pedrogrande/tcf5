    <meta itemprop="description" content="<%= content_for?(:description) ? yield(:description) : 'Coder Factory' %>">
    <meta itemprop="image" content="<%= content_for?(:image) ? yield(:image) : asset_path('logo22.png') %>">
    <!-- Twitter tags -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="@coderfactory">
    <meta name="twitter:title" content="<%= content_for?(:title) ? yield(:title) : 'Web Development Articles - Coder Factory Blog' %>">
    <meta name="twitter:description" content="<%= content_for?(:description) ? yield(:description) : 'Coder Factory' %>">
    <meta name="twitter:creator" content="<%= content_for?(:author) ? yield(:author) : '@coderfactory' %>">
    <meta name="twitter:image:src" content="<%= content_for?(:image) ? yield(:image) : asset_path('logo22.png') %>">
    <!-- Open Graph -->
    <meta property="og:title" content="<%= content_for?(:title) ? yield(:title) : 'Web Application Development Articles - Coder Factory Blog' %>" />
