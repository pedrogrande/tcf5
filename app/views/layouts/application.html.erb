$(document).ready(function() {
    $('#fullpage').fullpage({
        verticalCentered: false,
        resize : false,
        // slidesColor : ['#ccc', '#fff'],
        anchors:['home', 'coder-factory', 'course', 'schedule', 'team', 'faq', 'numbers', 'apply', 'contact'],
        scrollingSpeed: 1700,
        easing: 'easeOutSine',
        touchSensitivity: 6,
        css3 : true,
        fixedElements : '.navbar',
        scrollOverflow : false,
        paddingTop: 0
    });
});
</script>

