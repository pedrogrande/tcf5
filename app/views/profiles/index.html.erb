<h1>Listing profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Phone</th>
      <th>Profile picture</th>
      <th>User</th>
      <th>Slug</th>
      <th>Google plus</th>
      <th>Linkedin</th>
      <th>Twitter</th>
      <th>Github</th>
      <th>Website</th>
      <th>Title</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.profile_picture %></td>
        <td><%= profile.user %></td>
        <td><%= profile.slug %></td>
        <td><%= profile.google_plus %></td>
        <td><%= profile.linkedin %></td>
        <td><%= profile.twitter %></td>
        <td><%= profile.github %></td>
        <td><%= profile.website %></td>
        <td><%= profile.title %></td>
        <td><%= profile.info %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
