<%= simple_form_for(@special) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :admin_id %>
    <%= f.input :recipient_email %>
    <%= f.association :course %>
    <%= f.association :intake %>
    <%= f.input :dollar_discount %>
    <%= f.input :percent_discount %>
    <%= f.input :guid %>
    <%= f.input :date_redeemed %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
