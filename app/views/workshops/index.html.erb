<h1>Listing workshops</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Tagline</th>
      <th>Description</th>
      <th>Duration minutes</th>
      <th>Icon</th>
      <th>Skill group</th>
      <th>Workshop code</th>
      <th>Active</th>
      <th>Slug</th>
      <th>Outcome</th>
      <th>Objectives</th>
      <th>Benefits</th>
      <th>Skill level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workshops.each do |workshop| %>
      <tr>
        <td><%= workshop.name %></td>
        <td><%= workshop.tagline %></td>
        <td><%= workshop.description %></td>
        <td><%= workshop.duration_minutes %></td>
        <td><%= workshop.icon %></td>
        <td><%= workshop.skill_group %></td>
        <td><%= workshop.workshop_code %></td>
        <td><%= workshop.active %></td>
        <td><%= workshop.slug %></td>
        <td><%= workshop.outcome %></td>
        <td><%= workshop.objectives %></td>
        <td><%= workshop.benefits %></td>
        <td><%= workshop.skill_level %></td>
        <td><%= link_to 'Show', workshop %></td>
        <td><%= link_to 'Edit', edit_workshop_path(workshop) %></td>
        <td><%= link_to 'Destroy', workshop, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workshop', new_workshop_path %>
